{
  "name": "pg-accessor",
  "version": "1.0.1",
  "description": "A Node.js module for PostgreSQL that automatically generates accessors (getters and setters) for all tables in a database.",
  "keywords": [
    "postgresql",
    "pg",
    "postgres",
    "getter",
    "setter"
  ],
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development ./index.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha",
    "db:migrate": "NODE_ENV=development node ./lib/db/migration.js",
    "install": "npm run db:migrate"
  },
  "bin": {
    "accessor": "./index.js"
  },
  "author": "Uzair Inamdar <uzair_inamdar@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uzillion/pg-accessor"
  },
  "homepage": "https://github.com/uzillion/pg-accessor#readme",
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "pg-promise": "^8.4.5",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "mocha": "^5.2.0"
  }
}
